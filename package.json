{
  "name": "visu-congresso",
  "version": "1.0.0",
  "description": "visualização de gastos parlamentares",
  "main": "index.js",
  "dependencies": {
    "progressbar.js": "^1.0.1"
  },
  "devDependencies": {
    "jspm": "^0.17.0-beta.31",
    "jspm-hmr": "^0.5.0"
  },
  "jspm": {
    "name": "visu-congresso",
    "main": "visu-congresso.js",
    "dependencies": {},
    "devDependencies": {
      "crossfilter": "npm:crossfilter@^1.3.12",
      "d3": "npm:d3@4.3.0",
      "d3-tip": "npm:d3-tip@^0.7.1",
      "d3v3": "npm:d3@3.5.17",
      "jquery": "npm:jquery@^3.1.1",
      "jquery.scrollto": "npm:jquery.scrollto@^2.1.2",
      "papaparse": "npm:papaparse@^4.1.2",
      "progressbar.js": "npm:progressbar.js@^1.0.1",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0",
      "systemjs-plugin-babel": "npm:systemjs-plugin-babel@0.0.17"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "github:socketio/socket.io-client@1.5.1": {
        "main": "socket.io.js"
      },
      "npm:d3@3.5.17": {
        "meta": {
          "d3.js": {
            "exports": "d3",
            "format": "global"
          }
        }
      },
      "npm:debug@2.3.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery.scrollto@2.1.2": {
        "format": "global"
      },
      "npm:jquery@3.1.1": {
        "format": "amd"
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  },
  "scripts": {
    "packages": "npm install && jspm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorleao/meucongressonacional-visualizacao-2016-2.git"
  },
  "keywords": [
    "congresso",
    "congresso",
    "nacional",
    "visualização",
    "gastos",
    "opendata",
    "dados",
    "abertos"
  ],
  "author": "Caio, Igor e Paulo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/igorleao/meucongressonacional-visualizacao-2016-2/issues"
  },
  "homepage": "https://github.com/igorleao/meucongressonacional-visualizacao-2016-2#readme"
}
